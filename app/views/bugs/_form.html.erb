<h4>Edit bug:</h4>

<%= form_for([@project,@bug]) do |f| %>
<div class="field">
  <%= f.label :Name %><br />
  <%= f.text_field :name %>
</div>
<div class="field">
  <%= f.label :Content %><br />
  <%= f.text_area :body %>
</div>
<div class="field">
  <%= f.label :Status %><br />
  <%= f.text_field :status %>
</div>
<div class="actions">
  <%= f.submit %>
</div>
<% end %>
